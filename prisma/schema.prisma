datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "app/generated/prisma/client"
}


model Todo{
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  body      String?
  completed Boolean  @default(false)
  userid    String   
  createdAt DateTime @default(now())
  
}



